--1. Баардык продукталарды(Products) жана информацияларды чыгар.
select * from products join product_info pi on products.id = pi.product_id;
--2. Информациясы бар болгон продукталарды гана чыгар.
select  * from  product_info join products p on p.id = product_info.product_id;
-- 3. Информациясы жок(null) болгон продукталарды чыгар.
select  * from product_info join products p on p.id =  null;
-- 4. Баардык продукталардын атын жана срогун чыгар.
select  * from  products full join product_info pi on products.id = pi.product_id;
-- 5. Баардык продукталарды жана анын мамлекеттери "М" менен башталган шаарларын чыгар.
select  * from product_info full join products p on p.id = product_info.product_id where  country like 'M%';
-- 6. Баардык кардардын(Customers) аттарын жана кардарга тийешелуу карзинанын(Basket_терин) аттарын сорттоп чыгар.(asc,desc)
select  *from  customers full join basket b on customers.id = b.customer_id  order by basket_name;
select  *from  customers full join basket b on customers.id = b.customer_id  order by basket_name desc ;
-- 7. Кардардын аты жана кардардын продуктулары чыксын.
select first_name,name from customers join basket b on customers.id = b.customer_id join basket b2 on customers.id = b2.customer_id join products_basket pb on b2.id = pb.basket_id join products p on p.id = pb.products_id;
-- 8. Кардардын аты, кардар алган продуктун аты, баасы, салмагы чыксын.
select  first_name,name,price,weight from  customers join basket b on customers.id = b.customer_id join basket b2 on customers.id = b2.customer_id join products_basket pb on b2.id = pb.basket_id join products p on p.id = pb.products_id;
-- 9. Бир кардар эки окшош продукт алган болсо, кардардын аты,
--     кардар алган продукталарынын аты уникалдуу болуп баасы жана салмагы кошулуп чыгар.
select  first_name,name,price,weight from  customers join basket b on customers.id = b.customer_id
    join  basket b2 on customers.id = b2.customer_id
    join  products_basket pb on b2.id = pb.basket_id
    join  products  p on p.id = pb.products_id    ;
-- 10.Бир өлкөдө чыккан продукталдын баарын чыгар.
select  p.country from  products  join product_info pi on products.id = pi.product_id
    join product_info p on products.id = p.product_id
    join products_basket pb on products.id = pb.products_id
    join basket b on b.id = pb.basket_id join customers c on c.id = b.customer_id where  pi.country like '%China'
;
-- 11.Кардарлардын баарын жана алган продукталарынын баасынын суммасын тегеректеп (округлить) чыгар.
select  first_name ,sum(avg(price))from customers
join basket b on customers.id = b.customer_id
    join customers c on c.id = b.customer_id
    join basket b2 on customers.id = b2.customer_id
    join products_basket pb on b2.id = pb.basket_id join products_basket p on b.id = p.basket_id join products p2 on p2.id = p.products_id join product_info pi on p2.id = pi.product_id

-- 12."2021" ден "2023" кө чейинки сроктогу продукталардын атын жана срокторун чыгар.
select  name ,expiration_date from products join product_info pi on products.id = pi.product_id where  expiration_date between  date'1-1-2021'and date'1-1-2023';
-- 13.Срогу "2022" болгон продукталардын шаарын , продукталардын атын жана баасын чыгар бирок баалары 0 болуп калсын.
select  expiration_date ,name ,price=0 from products join product_info pi on products.id = pi.product_id where  expiration_date between date '2022-01-01'and date '2022-12-31';
-- 14.Баардык кардарлардын продукталарынын сумассын чыгар.
select  sum(price) from  customers join basket b on customers.id = b.customer_id
    join basket b2 on customers.id = b2.customer_id
    join products_basket pb on b2.id = pb.basket_id join products p on p.id = pb.products_id ;
-- 15.Кардардын аты жана ал сатып алган продуктунун аты, тусу чыксын.
select first_name, colour from  customers join basket b on customers.id = b.customer_id
    join basket b2 on customers.id = b2.customer_id
    join products_basket pb on b2.id = pb.basket_id
    join products p on p.id = pb.products_id
    join product_info pi on p.id = pi.product_id ;
-- 16.Баардык продуктулардын салмактарын чыгар.
select  sum(weight) from products;
-- 17.Кардарлардын email_дары "com" менен буткон кардарлардын аттары, корзинасынын аты чыксын.
select  last_name,basket_name,email from customers  join basket b on customers.id = b.customer_id where email like '%com';
-- 18.Продуклардын атын, кыскача маалыматын(description), кардардын атын чыгар качан кардарлардын номери жок(null) болсо.
select  discription,last_name from customers join basket b on customers.id = b.customer_id join basket b2 on customers.id = b2.customer_id join products_basket pb on b2.id = pb.basket_id join products p on p.id = pb.products_id where  phone_number = null;
-- 19.Бардык продуктунун тустору жана тустору окшош продукталары чыксын.
select  colour from product_info where colour
-- 20.Баардык продукталардын мамлекти окшош болгон продукталардын атын жана анын кардарларын атын чыгар.